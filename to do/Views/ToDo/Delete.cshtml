@model to_do.Models.ToDo
@{
    ViewData["Title"] = "Delete";
}


    <p>Are you sure you want to delete this todo item?</p>

    <form asp-action="DeletePost" method="post" onsubmit="return confirm('Are you sure you want to delete this todo item?');">
        <input type="hidden" asp-for="Id" />
        <button type="submit" class="btn btn-danger">Delete</button>
        <a asp-action="GetAll" class="btn btn-default">Cancel</a>
    </form>




